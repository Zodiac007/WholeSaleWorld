<% include header %>

<!-- ********* Checkout Form ************ -->

<div class="uk-section-default">
  <div
    class="uk-section uk-light uk-background-cover"
    style="background-image: url(Images/ContactPage4.jpg)"
  >
    <div class="uk-container">
        <div>
          <div
            class="uk-card uk-card-primary uk-card-hover uk-card-body uk-card-large uk-light"
          >
            <h2 class="uk-card-title" style="text-align:center;">
              Make Payment
            </h2>
            <form>
              <fieldset class="uk-fieldset">
                <legend class="uk-legend">Full Name</legend>
                <div class="uk-margin">
                  <div class="uk-inline uk-width-1-1">
                    <span class="uk-form-icon" uk-icon="icon: user"></span>
                    <input
                      class="uk-input"
                      type="text"
                      placeholder="Enter Your Name"
                    />
                  </div>
                </div>
                <legend class="uk-legend">Email</legend>
                <div class="uk-margin">
                  <div class="uk-inline uk-width-1-1">
                    <span class="uk-form-icon" uk-icon="icon: mail"></span>
                    <input
                      class="uk-input"
                      type="email"
                      placeholder="example@gmail.com"
                    />
                  </div>
                </div>
                <legend class="uk-legend">Message</legend>
                <div class="uk-margin">
                  <textarea
                    class="uk-textarea"
                    rows="5"
                    placeholder="Enter Message Here.."
                  ></textarea>
                </div>
                <a href="semester.html">
                  <button class="uk-button uk-button-default">Send</button>
                </a>
              </fieldset>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
